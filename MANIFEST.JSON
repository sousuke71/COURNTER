{
    "manifest_version": 2,
    "name": "Googling history",
    "description": "Display how many times did you google in a day.",
    "version": "1.0",
    "icons": {
        "16": "images/icon/icon16.png",
        "48": "images/icon/icon48.png",
        "128": "images/icon/icon128.png"
      },
    "browser_action": {
      "default_icon": "images/icon/icon19.png",
      "default_popup": "popup.html"
    },
    "permissions": [
      "history"
    ]
  }
